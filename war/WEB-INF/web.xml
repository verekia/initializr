<?xml version="1.0" encoding="UTF-8"?> 
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    version="2.5">

<context-param> 
 <param-name>com.sun.faces.enableThreading</param-name> 
 <param-value>false</param-value> 
 </context-param> 

  <servlet>
    <servlet-name>builderServlet</servlet-name>
    <servlet-class>com.jverrecchia.initializr.builder.BuilderServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>tryServlet</servlet-name>
    <servlet-class>com.jverrecchia.initializr.web.TryServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>builderServlet</servlet-name>
    <url-pattern>/builder</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>tryServlet</servlet-name>
    <url-pattern>/try</url-pattern>
  </servlet-mapping>    

	<welcome-file-list>
		<welcome-file>front/index.html</welcome-file>
	</welcome-file-list>

</web-app>